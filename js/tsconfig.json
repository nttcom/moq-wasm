{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["@types/dom-mediacapture-transform"],
    "lib": ["esnext", "webworker", "dom"]
  },
  "include": ["examples/**/*.ts", "examples/**/*.tsx", "examples/media/publisher/files/decoder.js"],
  "exclude": ["node_modules"]
}
