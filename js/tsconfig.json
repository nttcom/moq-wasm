{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["@types/dom-mediacapture-transform"]
  },
  "include": ["examples/**/*.ts", "examples/**/*.tsx"],
  "exclude": ["node_modules"]
}
